package view;

import model.Point;

/**
 * @author Danielle
 *
 */
public class TetrisBlock
{
    private final int myX;
    
    private final int myY;

    /**
     * 
     */
    public TetrisBlock(int theX, int theY)
    {
        myX = theX;
        myY = theY;
    }
    
    public int getX()
    {
        return myX;
    }
    
    public int getY()
    {
        return myY;
    }

}
